{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@finalproject/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "rewrites": [
      { 
        "source": "/(.*)", 
        "destination": "app.js" 
      }
    ]
  }